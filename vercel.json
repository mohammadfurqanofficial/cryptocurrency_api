{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "crons": [
        {
            "path": "cryptocurrency-api-red.vercel.app/api/coins/save-history",
            "schedule": "*/1 * * * *"
        }
    ]
}